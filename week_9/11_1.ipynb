   "source": [
    "library(repr)\n",
    "options(repr.plot.width = 6, repr.plot.height = 4)\n",
    "options(repr.plot.res = 200)\n",
    "\n",
    "library(ggplot2)\n",
    "\n",
    "set.seed(1)\n",
    "\n",
    "n <- 200\n",
    "p <- 0.7\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {
    "vscode": {
     "languageId": "r"
    }
   },
   "outputs": [
